    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "zip": "yarn run build && yarn run compress:7z || yarn run compress:zip || yarn run compress:rar",
    "compress:zip": "cd dist && zip -r dev.boa.isipass.com.bo.zip .",
    "compress:7z": "cd dist && 7z a -r dev.boa.isipass.com.bo.zip .",
    "compress:rar": "cd dist && rar a -r dev.boa.isipass.com.bo.rar ."
  },
